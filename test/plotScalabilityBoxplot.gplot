# Plot the execution times.

set style line 1 lc rgb "#d7191c"
set style line 2 lc rgb "#fdae61"

set style boxplot outliers pointtype 7
set style data boxplot
set boxwidth  0.5
set pointsize 0.5

set terminal png font "Verdana" 10
set ylabel "Average CPU%"

unset key
set border 2
set xtics ("C-SPARQL" 1, "CQELS" 2, "Query Streamer" 3) scale 0.0
set xtics nomirror
set ytics nomirror
set yrange [0:300]

plot '.scalability-100.data' using (1):1 title col ls 1, '' using (2):3 title col ls 3, '' using (3):2 title col ls 2
